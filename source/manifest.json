{
  "name": "The Old Reader",
  "description": "The ultimate RSS reader. Just like the old google reader, only better.",
  "version": "1",
  "manifest_version" : 2,
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "urls": [
      "http://theoldreader.com/"
    ],
    "launch": {
      "web_url": "http://theoldreader.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
